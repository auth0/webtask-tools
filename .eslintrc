{
    
    "POSSIBLE ERRORS": true,
    "comma-dangle": [2, "always-multiline"],
    "no-cond-assign": [2],
    "no-console": [0],
    "no-constant-condition": [0],
    "no-control-regex": [0],
    "no-debugger": [0],
    "no-dupe-args": [2],
    "no-duplicate-case": [2],
    "no-empty-character-class": [2],
    "no-empty": [2],
    "no-ex-assign": [2],
    "no-extra-boolean-cast": [1],
    "no-extra-parens": [1],
    "no-extra-semi": [1],
    "no-func-assign": [2],
    "no-inner-declarations": [2],
    "no-invalid-regexp": [2],
    "no-irregular-whitespace": [1],
    "no-negated-in-lhs": [2],
    "no-obj-calls": [2],
    "no-regex-spaces": [1],
    "no-sparse-arrays": [2],
    "no-unexpected-multiline": [2],
    "no-unreachable": [1],
    "use-isnan": [2],
    "valid-jsdoc": [1],
    "valid-typeof": [2],
    
    "BEST PRACTICES": true,
    "dot-location": [1, "property"],
    "dot-notation": [0],
    "eqeqeq": [2],
    "no-case-declarations": [2],
    "no-div-regex": [2],
    "no-else-return": [1],
    "no-eq-null": [2],
    "no-extend-native": [2],
    "no-extra-bind": [1],
    "no-floating-decimal": [2],
    "no-invalid-this": [1],
    "no-loop-func": [1],
    "no-magic-numbers": [1],
    "no-multi-str": [2],
    "no-native-reassign": [2],
    "no-new": [2],
    "no-redeclare": [1],
    "no-return-assign": [1],
    "no-self-compare": [2],
    "no-sequences": [2],
    "no-throw-literal": [2],
    "no-unused-expressions": [1],
    "no-useless-call": [1],
    "no-useless-concat": [1],
    "no-void": [2],
    "no-with": [2],
    "radix": [2],
    "vars-on-top": [0],
    
    "VARIABLES": true,
    "no-catch-shadow": [1],
    "no-delete-var": [2],
    "no-shadow-restricted-names": [2],
    "no-shadow": [1],
    "no-unused-vars": [1],
    "no-use-before-define": [2],
    
    "NODE.JS AND COMMONJS": true,
    "callback-return": [2, ["callback", "cb", "next"]],
    "handle-callback-err": [1],
    "no-new-require": [2],
    "no-path-concat": [1],
    "no-sync": [1],
    
    "STYLISTIC ISSUES": true,
    "array-bracket-spacing": [1, "never"],
    "block-spacing": [1, "always"],
    "brace-style": [1, "1tbs"],
    "camelcase": [1, { properties: "always", }],
    "comma-spacing": [1, { before: false, after: true }],
    "indent": [1, 4, { SwitchCase: 1, VariableDeclarator: 1 }],
    "key-spacing": [1, { beforeColon: false, afterColon: true, mode: "minimum" }],
    "new-cap": [2],
    "new-parens": [2],
    "newline-after-var": [1, "always"],
    "no-lonely-if": [1],
    "no-mixed-spaces-and-tabs": [2],
    "no-spaced-func": [2],
    "no-underscore-dangle": [2],
    "no-unneeded-ternary": [1],
    "object-curly-spacing": [1, "always"],
    "one-var": [1, "never"],
    "quote-props": [1, "consistent-as-needed"],
    "quotes": [1, "single", "avoid-escape"],
    "semi-spacing": [1, { before: false, after: true }],
    "semi": [1, "always"],
    "sort-vars": [1, { ignoreCase: false }],
    "space-after-keywords": [1, "always"],
    "space-before-blocks": [1, "always"],
    "space-before-function-parent": [1, { anonymous: "always", named: "never "}],
    "space-before-keywords": [1, "always"],
    "space-in-parens": [1, "never"],
    "space-infix-ops": [1],
}
